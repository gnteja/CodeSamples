<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name = "EntityTobeAutowired" class="com.learngn.springdependencyinjection.entity.EntityTobeAutowired">
		<property name="name" value="#########################"></property>
	</bean>
	
	<bean name="AutowireServicebyName"
	class="com.learngn.springdependencyinjection.service.AutowireService"
	autowire="byName"/>
	
	<bean name="AutowireServicebyType"
	class="com.learngn.springdependencyinjection.service.AutowireService"
	autowire="byType"/>
	
	<bean name="AutowireServicebyConstructor"
	class="com.learngn.springdependencyinjection.service.AutowireService"
	autowire="constructor"/>


</beans>
